
public class Empleado {
	
	
	
	/*De cada empleado nos interesa: nombre de la empresa, nombre del
	empleado, edad, porcentaje de incremento de sueldo y sueldo base. */
	
	private final String nombreEmpresa= "Electricas";
	private final int sueldo = 1000;
	private double porcentaje=0;
	private String nombre="";
	private int edad =0;
	public Empleado(int porcentaje, String nombre, int edad) {
		
		this.porcentaje = porcentaje;
		this.nombre = nombre;
		this.edad = edad;
	}
	public double getPorcentaje() {
		return porcentaje;
	}
	public void setPorcentaje(double porcentaje) {
		this.porcentaje = porcentaje;
	}
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	public int getEdad() {
		return edad;
	}
	public void setEdad(int edad) {
		this.edad = edad;
	}
	@Override
	public String toString() {
		return "Empleado [nombreEmpresa=" + nombreEmpresa + "\n sueldobase=" + sueldo + "\n porcentaje=" + porcentaje
				+ "\n nombre=" + nombre + "\n edad=" + edad + "]";
	}
	
	
	
	
	
	
	


}
