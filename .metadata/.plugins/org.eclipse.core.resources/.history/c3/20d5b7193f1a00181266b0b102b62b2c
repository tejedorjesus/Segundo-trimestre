
public class paises {
	
	
	private String nombre;
	private static int contapais=1;

	public paises(String nombre) {
		contapais++;
		this.nombre = nombre;
		
	}
	



	public int getContapais() {
		return contapais;
	}




	public void setContapais(int contapais) {
		this.contapais = contapais;
	}




	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	@Override
	public String toString() {
		return " [ " + nombre + "]";
	} 

}
