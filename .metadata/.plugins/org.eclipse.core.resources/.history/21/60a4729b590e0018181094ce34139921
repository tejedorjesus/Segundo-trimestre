		// TODO Auto-generated method stub
		
		
		int Array []= new int[4];
		int num=0;
		Scanner sc = new Scanner(System.in);
		for (int i = 0; i < Array.length; i++) {
			System.out.println("intro num");
			num=sc.nextInt();
			num=Array[i];
			
			
			
		}
		for (int i = 0; i < Array.length; i++) {
		System.out.println(i);

	}
	}
}
