import java.util.Scanner;
public class HoraMain {

	public static void main(String[] args) {

		int horas=0;
		int minutos = 0;
		int segundos=0;



		Hora reloj =null;
		reloj=null;
		
		
		
		reloj=new Hora (horas, minutos, segundos);
		reloj.PonerHora(horas, minutos, segundos);
		
		
		System.out.println(reloj.visualizar());
		




	}

}
